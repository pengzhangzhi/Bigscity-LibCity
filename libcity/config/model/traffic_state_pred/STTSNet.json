{
  "close_dim": 64,
  "trend_dim": 64,
  "close_head": 2,
  "trend_head": 2,
  "conv_channels": 64,
  "patch_size": [10,10],
  "close_depth": 2,
  "trend_depth": 2,
  "close_mlp_dim": 2,
  "trend_mlp_dim": 2,
  "pre_conv": true,
  "drop_prob": 0.1,
  "shortcut": true,

  "scaler": "minmax11",
  "load_external": true,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": false,
  "add_day_in_week": false,
  "external_time": false,

  "max_epoch": 500,
  "learner": "adam",
  "learning_rate": 0.0002,
  "lr_decay": true,
  "clip_grad_norm": false,
  "max_grad_norm": 0.1,
  "use_early_stop": true,
  "patience": 50
}
